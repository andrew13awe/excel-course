{
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "linebreak-style": ["error", "windows"]
    }
}